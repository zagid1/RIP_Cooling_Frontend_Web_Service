{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "identifier": "com.coolsystems.app",
  "productName": "front",
  "version": "0.1.0",
  "app": {

    "windows": [
      {
        "title": "front",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "devtools": true
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": true
  },
  "build": {
    "frontendDist": "../dist",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build:tauri"
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  },
  "plugins": {
    "http": {
      "scope": [
        "http://localhost:8080/*",
        "http://localhost:8090/*",
        "http://10.77.189.51:8080/*"
      ],
      "dangerousAcceptAnyServerCertificate": true
    }
  }
}
